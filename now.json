{
  "version": 2,
  "name": "lg_online",
  "alias": [
    "www.lgonline.cl"
  ],
  "builds": [{ "src": "package.json", "use": "@now/next" }],
  "routes": [
    {
      "src": "/$",
      "dest": "/browse",
      "headers": { "cache-control": "s-maxage=14400" }
    },
    {
      "src": "/did$",
      "dest": "/landing?landing=did",
      "headers": { "cache-control": "s-maxage=14400" }
    },
    {
      "src": "/instantpartysummer$",
      "dest": "/landing?landing=instantpartysummer",
      "headers": { "cache-control": "s-maxage=14400" }
    },
    {
      "src": "/twinwash$",
      "dest": "/landing?landing=twinwash",
      "headers": { "cache-control": "s-maxage=14400" }
    },
    {
      "src": "/oportunidadesdelasemana$",
      "dest": "/landing?landing=oportunidadesdelasemana",
      "headers": { "cache-control": "s-maxage=14400" }
    },
    {
      "src": "/instaviewbundle$",
      "dest": "/landing?landing=instaviewbundle",
      "headers": { "cache-control": "s-maxage=14400" }
    },
    {
      "src": "/neobundle$",
      "dest": "/landing?landing=neobundle",
      "headers": { "cache-control": "s-maxage=14400" }
    },
    {
      "src": "/planperfecto$",
      "dest": "/landing?landing=planperfecto",
      "headers": { "cache-control": "s-maxage=14400" }
    },
    {
      "src": "/oledn1$",
      "dest": "/landing?landing=oledn1",
      "headers": { "cache-control": "s-maxage=14400" }
    },
    {
      "src": "/lgweek$",
      "dest": "/landing?landing=lgweek",
      "headers": { "cache-control": "s-maxage=14400" }
    },
    {
      "src": "/modernizate$",
      "dest": "/landing?landing=modernizate",
      "headers": { "cache-control": "s-maxage=14400" }
    },
    {
      "src": "/especial_lavado$",
      "dest": "/landing?landing=especial_lavado",
      "headers": { "cache-control": "s-maxage=14400" }
    },
    {
      "src": "/(?<section>[^/]+)$",
      "dest": "/browse?section=$section",
      "headers": { "cache-control": "s-maxage=14400" }
    },
    {
      "src": "/products/(?<product>[^/]+?)-(?<slug>[^/]+)$",
      "dest": "/products?product=$product&slug=$slug",
      "headers": { "cache-control": "s-maxage=14400" }
    },
    {
      "src": "/products/(?<product>[^/]+?)$",
      "dest": "/products?product=$product",
      "headers": { "cache-control": "s-maxage=14400" }
    }
  ]
}
